{
  "name": "builderbook-public",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server/app.js --watch server --exec babel-node",
    "build": "npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "babel server -d compiled/server && cp -R static compiled",
    "start": "node compiled/server/app.js",
    "lint": "eslint components pages lib server",
    "test": "jest --coverage",
    "ln": "now ln <url> builderbook.org",
    "rm": "now rm builderbook-public --safe --yes",
    "now-upgrade": "npm install now -g",
    "processes": "ss -lptn 'sport = :8000'",
    "kill-pud": "kill -9 <pid>"
  },
  "jest": {
    "coverageDirectory": "./.coverage"
  },
  "dependencies": {
    "@octokit/rest": "14.0.5",
    "aws-sdk": "^2.155.0",
    "babel-cli": "^6.26.0",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "front-matter": "^2.3.0",
    "googleapis": "^29.0.0",
    "handlebars": "^4.0.11",
    "he": "^1.1.1",
    "helmet": "^3.12.0",
    "highlight.js": "^9.12.0",
    "htmlescape": "^1.1.1",
    "isomorphic-unfetch": "^2.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.2",
    "marked": "^0.3.12",
    "material-ui": "1.0.0-beta.41",
    "mongoose": "^5.0.0-rc2",
    "next": "^5.1.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-jss": "^8.1.0",
    "react-stripe-checkout": "^2.6.3",
    "request": "^2.83.0",
    "sitemap": "^1.13.0",
    "stripe": "^5.3.0",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.1",
    "nodemon": "^1.17.3"
  }
}
